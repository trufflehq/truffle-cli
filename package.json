    "url": "https://github.com/trufflehq/truffle-cli/issues"
  },
  "homepage": "https://github.com/trufflehq/truffle-cli#readme",
  "dependencies": {
    "@deno/eszip": "^0.22.0",
    "arraybuffer-to-buffer": "^0.0.7",
    "chalk": "^5.0.1",
    "commander": "^9.3.0",
    "esm": "https://github.com/jsg2021/esm/releases/download/v3.x.x-pr883/esm-3.x.x-pr883.tgz",
    "form-data": "^4.0.0",
